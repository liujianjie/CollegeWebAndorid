<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="7dp">
    
    <com.example.qq_lxr9y18r.RoundView
        android:id="@+id/item_ivz"
        android:layout_width="50dp"
        android:layout_height="45dp"
        />
    <TextView 
        android:id="@+id/item_ivz_text"
        android:textSize="12dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/item_ivz"
        android:layout_marginTop="-6dp"
       />
	<com.example.qq_lxr9y18r.RoundView
        android:id="@+id/item_ivy"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:layout_alignParentRight="true"
       />
   <TextView 
        android:id="@+id/item_ivy_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12dp"
        android:textColor="#ffffff"
        android:layout_toLeftOf="@id/item_ivy"
        android:layout_marginTop="-6dp"
       />
</RelativeLayout>
